{
  "name": "conversiontools",
  "version": "2.0.0",
  "description": "Modern TypeScript/JavaScript library to convert files using Conversion Tools API at https://conversiontools.io - supports ESM & CommonJS with full TypeScript types",
  "keywords": [
    "conversion",
    "tools",
    "convert",
    "document",
    "file",
    "api",
    "sdk",
    "client",
    "typescript",
    "xml",
    "json",
    "excel",
    "pdf",
    "csv"
  ],
  "homepage": "https://github.com/conversiontools/conversiontools-node#readme",
  "bugs": {
    "url": "https://github.com/conversiontools/conversiontools-node/issues",
    "email": "info@conversiontools.io"
  },
  "license": "MIT",
  "author": "Conversion Tools <info@conversiontools.io>",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./legacy": {
      "import": {
        "types": "./dist/legacy.d.ts",
        "default": "./dist/legacy.js"
      },
      "require": {
        "types": "./dist/legacy.d.cts",
        "default": "./dist/legacy.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/conversiontools/conversiontools-node.git"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run typecheck && npm run build"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "tsup": "^8.5.1",
    "typescript": "^5.3.3",
    "vitest": "^4.0.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
